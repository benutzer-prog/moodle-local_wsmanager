{{!
    @template local_wsmanager/dashboard
    Dashboard output
}}
<div class="alert alert-secondary alert-block fade in">
    <a href="{{ globals.config.wwwroot }}/local/wsmanager/webservices.php" class="btn btn-link"
       title="{{#str}} webservices, webservice {{/str}}">{{#pix}}
        i/db, core {{/pix}} {{#str}} webservices, webservice {{/str}}</a>
    <a href="{{ globals.config.wwwroot }}/local/wsmanager/webservices.php#create_webservice_form" class="btn btn-link"
       title="{{#str}} addaservice, webservice {{/str}}">{{#pix}}
        t/add, core {{/pix}} {{#str}} addaservice, webservice {{/str}}</a>
</div>
<div id="local_wsmanager_dashboard_info">
    {{{ dashboard_info }}}
</div>
<h2>{{#str}} webservicesoverview, webservice {{/str}}</h2>
{{{ overview_table }}}
<div class="local_wsmanager_state_wrapper"
     id="local_wsmanager_mobileapp_wrapper"{{#mobileappwrapperattrs_style}} {{ mobileappwrapperattrs_style }}{{/mobileappwrapperattrs_style}}>
    <h2>{{#str}} mobileapp, tool_mobile {{/str}}</h2>
    <div id="local_wsmanager_webservice_dashboard_mobile_info_output">{{{ mobile_info }}}</div>
    {{{ mobile_app_table }}}
</div>
<div class="local_wsmanager_state_wrapper"
     id="local_wsmanager_testing_wrapper"{{#testingwrapperattrs_style}} {{ testingwrapperattrs_style }}{{/testingwrapperattrs_style}}>
    <h2>{{#str}} testing, local_wsmanager {{/str}}</h2>
    {{{ testing_table }}}
    <div id="local_wsmanager_webservice_test_dashboard_table_output">
        <div class="local_wsmanager_bordered">
            {{{ test_webservice_function_table }}}
        </div>
    </div>
</div>
<h2>{{#str}} doc, webservice {{/str}}</h2>
{{{ documentation }}}