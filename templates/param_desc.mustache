{{!
    @template local_wsmanager/param_desc
    Webservice function param description info
}}
{{#desc}}<strong>{{ desc }}</strong>{{/desc}}
{{#listfilled}}
    <ul class="list-unstyled m-0" style="margin:0">
        {{#list}}
            {{#required}}
                <li>{{#str}}required, moodle{{/str}}: <code>{{#str}}yes, moodle{{/str}}</code></li>{{/required}}
            {{#default}}
                <li>{{#str}}default, moodle{{/str}}: {{ default }}</li>{{/default}}
            {{#type}}
                <li>{{#str}}type, mnet{{/str}}: <code>{{ type }}</code></li>{{/type}}
            {{#allownull}}
                <li>{{#str}}allow, mnet{{/str}} NULL: <code>{{#str}}yes, moodle{{/str}}</code></li>{{/allownull}}
        {{/list}}
    </ul>
{{/listfilled}}