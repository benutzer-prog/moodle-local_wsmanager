{{!
    @template local_wsmanager/check_webservices
    Webservices status badge status
}}
<div class="local_wsmanager_enablewebservices_check_wrapper">
    <div class="local_wsmanager_enablewebservices_check" id="local_wsmanager_enablewebservices_check_ok"
        {{^enablewebservices}} style="display:none"{{/enablewebservices}}>{{{ ok_str }}}</div>
    <div class="local_wsmanager_enablewebservices_check" id="local_wsmanager_enablewebservices_check_error"
        {{#enablewebservices}} style="display:none"{{/enablewebservices}}>{{{ error_str }}}</div>
</div>